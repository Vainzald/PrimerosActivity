<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.ejercicios">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Ejercicios"
        tools:targetApi="31">
        <activity
            android:name=".EjemplosAIEP"
            android:exported="false" />
        <activity
            android:name=".Conversor"
            android:exported="false"
            android:label="@string/name_conversor" />
        <activity
            android:name=".Contador"
            android:exported="false"
            android:label="@string/name_contador" />
        <activity
            android:name=".Saludar"
            android:exported="false"
            android:label="@string/name_saludar" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/name_menu">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>